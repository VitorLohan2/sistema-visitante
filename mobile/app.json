{
  "expo": {
    "name": "Liberaê",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "SUA_API_KEY_GOOGLE_MAPS_IOS"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.vitorlohanmatos.mobile",
      "config": {
        "googleMaps": {
          "apiKey": "SUA_API_KEY_GOOGLE_MAPS_ANDROID"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Precisamos acessar sua câmera para escanear códigos de barras"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite acesso à localização para registrar o ponto de trabalho.",
          "locationWhenInUsePermission": "Permite acesso à localização enquanto o app está em uso."
        }
      ]
    ],
    "extra": {
      "API_URL": "http://192.168.10.80:3001",
      "eas": {
        "projectId": "66a0d360-212e-4be5-8297-c521d0a51cfb"
      }
    }
  }
}
